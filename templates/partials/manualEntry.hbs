<div class="athena-entry">
    <div class="athena-table-row">
        <div class="athena-td1">
            <p>Manual</p>
        </div>
        <div class="athena-td2">
            <input type="text" class="path-field" name="manualPath" value="{{manualPath}}"/>
        </div>
        <div class="athena-td3">
            <input type="text" name="label" value="{{label}}" style="width: 100px;" />
        </div>
        <div class="centerChildren athena-td4">
            <input type="checkbox" id="something" value="default">
        </div>
        
        <div class="athena-delete-button-position">
            <button type="button" class="athena-button athena-delete-row" data-tooltip="Delete">
                <i class="fas fa-trash"></i>
            </button>
        </div>
    </div>
</div>  